<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="#F1F1F1">

    <net.wash8.customview.CustomTitle
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:background="#ffffff">

        <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="17sp"
                android:text="预约成功!"
                android:gravity="center"
                android:paddingTop="20dp"
                android:textStyle="bold"
                android:textColor="@color/submit_bg"/>

        <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="17sp"
                android:text="订单号"
                android:gravity="center"
                android:paddingTop="10dp"
                android:paddingBottom="20dp"
                android:textColor="@color/submit_bg"/>
    </LinearLayout>

    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:background="#ffffff"
            android:orientation="vertical">

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:layout_marginTop="16dp">

            <ImageView
                    android:id="@+id/iv_ordertime"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:background="@drawable/icon_ordertime_paypage"/>

            <TextView
                    android:id="@+id/tv_yuyueshijian"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="预约时间"
                    android:textSize="15sp"
                    android:layout_toRightOf="@+id/iv_ordertime"
                    android:textColor="@color/sliver_gray"
                    android:layout_marginLeft="20dp"/>

            <TextView
                    android:id="@+id/tv_ordertime"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="text"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@+id/tv_yuyueshijian"
                    android:textSize="15sp"/>
        </LinearLayout>

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="16dp"
                android:gravity="center_vertical">

            <ImageView
                    android:id="@+id/iv_washtime"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:background="@drawable/icon_washplace_paypage"/>

            <TextView
                    android:id="@+id/tv_dizhi"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="地       址"
                    android:textSize="15sp"
                    android:layout_toRightOf="@+id/iv_washtime"
                    android:textColor="@color/sliver_gray"
                    android:layout_marginLeft="20dp"/>

            <TextView
                    android:id="@+id/tv_orderplace"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:text="text"
                    android:layout_alignLeft="@+id/tv_dizhi"
                    android:layout_below="@+id/tv_dizhi"
                    android:textSize="15sp"/>
        </LinearLayout>

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="16dp"
                android:gravity="center_vertical">

            <ImageView
                    android:id="@+id/iv_washname"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:background="@drawable/icon_washname_paypage"/>

            <TextView
                    android:id="@+id/tv_chezhuxingming"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="车主姓名"
                    android:textSize="15sp"
                    android:layout_toRightOf="@+id/iv_washname"
                    android:textColor="@color/sliver_gray"
                    android:layout_marginLeft="20dp"/>

            <TextView
                    android:id="@+id/tv_username"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_alignLeft="@+id/tv_chezhuxingming"
                    android:layout_below="@+id/tv_chezhuxingming"
                    android:textSize="15sp"/>
        </LinearLayout>

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="16dp"
                android:gravity="center_vertical">

            <ImageView
                    android:id="@+id/iv_washphpne"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:background="@drawable/icon_washphone_paypage"/>

            <TextView
                    android:id="@+id/tv_chezhudianhua"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="车主电话"
                    android:textSize="15sp"
                    android:layout_toRightOf="@+id/iv_washphpne"
                    android:textColor="@color/sliver_gray"
                    android:layout_marginLeft="20dp"/>

            <TextView
                    android:id="@+id/tv_usermobile"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:text="text"
                    android:layout_alignLeft="@+id/tv_chezhudianhua"
                    android:layout_below="@+id/tv_chezhudianhua"
                    android:textSize="15sp"/>
        </LinearLayout>

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="16dp"
                android:paddingBottom="16dp"
                android:gravity="center_vertical">

            <ImageView
                    android:id="@+id/iv_washcar"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:background="@drawable/icon_washcar_paypage"/>

            <TextView
                    android:id="@+id/tv_cheliang"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="车       辆"
                    android:textSize="15sp"
                    android:layout_toRightOf="@+id/iv_washcar"
                    android:textColor="@color/sliver_gray"
                    android:layout_marginLeft="20dp"/>

            <TextView
                    android:id="@+id/tv_carmodel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="text"
                    android:layout_alignLeft="@+id/tv_cheliang"
                    android:layout_below="@+id/tv_cheliang"
                    android:layout_marginLeft="10dp"
                    android:textSize="15sp"
                    />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#ffffff"
            android:layout_marginTop="15dp">

        <LinearLayout
                android:paddingTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

            <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/icon_detail"/>

            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="已选择项目"
                    android:textSize="15sp"
                    android:textColor="@color/sliver_gray"
                    android:layout_marginLeft="20dp"/>
        </LinearLayout>

        <net.wash8.customview.LinearLayoutForListView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                />
        <!--        <RelativeLayout
                        android:paddingTop="20dp"
                        android:paddingLeft="60dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="text"
                            />
                    <TextView
                            android:layout_gravity="right"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:paddingRight="20dp"
                            android:text="text"/>
                </RelativeLayout>-->
    </LinearLayout>

</LinearLayout>